{
  "name": "golem-addr-cli",
  "version": "1.0.0",
  "description": "Vanity address generator CLI",
  "main": "dist/index.js",
  "bin": {
    "golem-addr": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint . --ignore-pattern 'dist/*'",
    "lint:fix": "eslint --fix . --ignore-pattern 'dist/*'",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "vanity",
    "address",
    "generator",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.28.0",
    "jest": "^29.5.0",
    "prettier": "3.5.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "dependencies": {
    "@golem-sdk/golem-js": "^3.5.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.41.0",
    "@opentelemetry/sdk-node": "^0.45.0",
    "commander": "^11.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  }
}
