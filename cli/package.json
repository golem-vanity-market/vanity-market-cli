{
  "name": "@unoperate/golem-vaddr-cli",
  "version": "0.1.1",
  "description": "Vanity address generator CLI",
  "main": "dist/index.js",
  "bin": {
    "golem-addr": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "generate": "ts-node ../tools/generate.ts",
    "dev": "ts-node src/index.ts generate --public-key sample-key.pub --vanity-address-prefix 0x44448888 --budget-glm 1 --results-file results.json",
    "crunch": "ts-node src/crunch.ts",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "list-cpu-offers": "node forward.js golem-sdk market scan",
    "list-gpu-offers": "node forward.js golem-sdk market scan --engine vm-nvidia"
  },
  "keywords": [
    "vanity",
    "address",
    "generator",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@golem-sdk/cli": "^2.11",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.18.1",
    "eslint": "^9.28.0",
    "jest": "^29.5.0",
    "prettier": "3.5.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "dependencies": {
    "@golem-sdk/golem-js": "^3.5.0",
    "@golem-sdk/pino-logger": "^1.1.0",
    "@opentelemetry/api": "~1",
    "@opentelemetry/auto-instrumentations-node": "~0.57",
    "@opentelemetry/sdk-logs": "~0.57",
    "@opentelemetry/sdk-metrics": "~1.30",
    "@opentelemetry/sdk-node": "~0.57",
    "@opentelemetry/sdk-trace-node": "~1.30",
    "cli-progress": "^3.12.0",
    "commander": "^11.0.0",
    "dotenv": "^16.5.0",
    "ethers": "^6.14.3",
    "pino": "^9.7.0",
    "rxjs": "^7.8.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  }
}
